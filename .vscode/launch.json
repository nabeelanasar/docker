{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Docker: Attach to nodecicd",
            "type": "node",
            "request": "attach",
            "remoteRoot": "/work/src/",
            "port": 9001,
            "address": "localhost",
            "localRoot": "${workspaceFolder}/node-cicd-repo/src/",
