        {
            "name": "Docker: Attach to nodeexpress",
            "type": "node",
            "request": "attach",
            "remoteRoot": "/work/src/",
            "port": 9002,
            "address": "localhost",
            "localRoot": "${workspaceFolder}/node-express-repo/src/",
            "protocol": "inspector",
            "restart": true
        }
    ]
}